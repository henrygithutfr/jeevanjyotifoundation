<section class="history-page" id="historyPage">
    <div class="history-page-container">
        <div class="page-heading">
            <h1 class="heading-text"><%- history[0].hisTitle %></h1>
            <p class="heading-paragraph"><%- history[0].hisDescription %></p>
        </div>

        <!-- Founding Story -->
        <%- history[0].foundingStory %>

        <!-- Timeline Section -->
        <%- history[0].timeline %>

        <!-- Milestones Section -->
        <%- history[0].milestones %>

        <!-- Growth Section -->
        <%- history[0].growth %>

        <!-- Legacy Section -->
        <%- history[0].legacy %>

        <!-- Future Vision -->
        <%- history[0].futureVision %>
    </div>
</section>